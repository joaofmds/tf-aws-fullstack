environment = "dev"
vpc_cidr    = "10.10.0.0/16"

public_subnets = {
  a = { cidr = "10.10.1.0/24", az = "us-east-1a" }
  b = { cidr = "10.10.2.0/24", az = "us-east-1b" }
}

private_app_subnets = {
  a = { cidr = "10.10.11.0/24", az = "us-east-1a" }
  b = { cidr = "10.10.12.0/24", az = "us-east-1b" }
}

private_db_subnets = {
  a = { cidr = "10.10.21.0/24", az = "us-east-1a" }
